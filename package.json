{
  "name": "patirti",
  "version": "4.9.5",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "cp:staging:android": "appcenter codepush release-react -a clockworkagency/patirti -d Staging -o ./android/build",
    "cp:production:android": "appcenter codepush release-react -a clockworkagency/patirti -d Production -o ./android/build",
    "cp:production:ios": "appcenter codepush release-react -a clockworkagency/Patirti-iOS -d Production -t 5.1.0",
    "sentry:sourcemaps": "sentry-cli releases files 4.9.5-codepush:v44 upload-sourcemaps --log-level=info --dist 4940 --strip-prefix . --rewrite ./android/build/CodePush",
    "cpsentry:stag": "npm run cp:staging && npm run sentry:sourcemaps",
    "cpsentry:prod": "npm run cp:production && npm run sentry:sourcemaps",
    "clean": "cd ./android && gradlew clean",
    "release": "react-native run-android --variant=release",
    "bundle": "cd ./android && gradlew bundleRelease"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
